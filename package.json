{
  "name": "dine_align",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^9.1.0",
    "mongoose": "^8.8.2"
  },
  "scripts": {
    "client": "npm start --prefix client",
    "server": "npm start --prefix server",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "gcp-build": "cd ./server && npm install && (rm -rf ./clientbuild || true) && cd ../client && npm install && npm run build && mv ./build ../server/clientbuild",
    "start": "npm start --prefix server"
  },
  "devDependencies": {
    "@types/express": "^5.0.0"
  }
}
